{
  "name": "vscode-hill",
  "displayName": "vscode-hill",
  "description": "Hill language support",
  "version": "0.0.1",
  "publisher": "hill-lang",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hill-lang/vscode-hill.git"
  },
  "contributes": {
    "languages": [{
      "id": "hill",
      "aliases": ["Hill", "hill"],
      "extensions": [".hill"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "hill",
      "scopeName": "source.hill",
      "path": "./syntaxes/hill.tmLanguage.json"
    }]
  }
}
